<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Picker">
        <!--竖向间距-->
        <attr name="lineSpaceMultiplier" format="integer" />
        <!--文字大小-->
        <attr name="textSize" format="dimension" />
        <!--旁边文字颜色 焦点文字颜色-->
        <attr name="textColorNormal" format="color" />
        <attr name="textColorFocus" format="color" />
        <!--//分割线-->
        <attr name="dividerWidthRatio" format="float" />
        <attr name="dividerColor" format="color" />
        <!--可见项数量-->
        <attr name="visibleItemCount" format="integer" />
        <!--能否循环-->
        <attr name="cycleable" format="boolean" />
    </declare-styleable>
</resources>